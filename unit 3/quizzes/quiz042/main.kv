ScreenManager:
    id:screenmanager
    MysteryPageA:
        name: "MysteryPageA"
    MysteryPageB:
        name: "MysteryPageB"
    MysteryPageC:
        name: "MysteryPageC"

<MysteryPageC>:
    MDLabel:
        text: "This is page C"
    MDRaisedButton:
        text: "go back"
        on_press: app.root.current = "MysteryPageB"

<MysteryPageA>:
    name: "MysteryPageA"
    MDBoxLayout:
        orientation: 'vertical'
        MDLabel:
            id: label1
            text: "Welcome"
            halign: "center"
        MDRaisedButton:
            text: "Click Me"
            on_press: root.message1()
            on_press: app.root.current = "MysteryPageC"

<MysteryPageB>:
    name: "MysteryPageB"
    MDBoxLayout:
        orientation: 'vertical'
        MDLabel:
            text: "This is mystery page B you pressed the button"
            id: label2
            halign: "center"
        MDRaisedButton:
            text: "Click Me"
            on_press: root.message2()
            on_press: app.root.current = "MysteryPageA"
